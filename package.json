{
  "name": "@brickdoc/monorepo",
  "version": "0.0.0",
  "author": "Brickdoc LTD",
  "description": "Brickdoc Monorepo Root",
  "private": true,
  "workspaces": [
    "gems/*",
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "postinstall": "husky install && ./bin/pdftron static",
    "start": "yarn design-colors build && ./bin/vite dev",
    "dist": "yarn design-colors build && ./bin/vite build",
    "dist:prod": "RAILS_ENV=production yarn dist",
    "dist:test": "RAILS_ENV=test yarn dist",
    "analyze": "BUNDLE_ANALYZER=1 yarn dist:prod",
    "lint": "yarn lint:eslint && yarn lint:rubocop && yarn lint:brakeman && yarn lint:packwerk",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:rubocop:fix",
    "lint:eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "lint:eslint:fix": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./",
    "lint:stylelint": "stylelint --cache **/*.css **/*.less",
    "lint:stylelint:fix": "stylelint --cache --fix **/*.css **/*.less",
    "lint:rubocop": "bundle exec rubocop --parallel --config .rubocop.yml",
    "lint:rubocop:fix": "bundle exec rubocop --config .rubocop.yml -a",
    "lint:brakeman": "bundle exec brakeman --no-pager --exit-on-warn",
    "tsc:check": "tsc --build",
    "tsc:build": "tsc --build tsconfig.build.json",
    "test": "yarn tsc:check && yarn lint && yarn test:jest && yarn test:rspec",
    "test:jest": "yarn tsc:build && jest --runInBand",
    "test:cypress": "cypress open",
    "test:rspec": "bundle exec rspec",
    "commit": "git add . && cz",
    "graphql": "bundle exec rails graphql:schema:dump && graphql-codegen --config codegen.yml",
    "cleanup": "rm -rf node_modules/.cache && rm -rf dist && rm -rf public/esm-bundle && rm -rf public/packs-test && rm -rf junit-reports && rm -rf coverage && yarn design-icons cleanup && yarn design-colors cleanup && yarn design-system cleanup && yarn editor cleanup && yarn uploader cleanup && yarn formula cleanup",
    "design-icons": "yarn workspace @brickdoc/design-icons",
    "design-colors": "yarn workspace @brickdoc/design-colors",
    "design-system": "yarn workspace @brickdoc/design-system",
    "editor": "yarn workspace @brickdoc/editor",
    "formula": "yarn workspace @brickdoc/formula",
    "uploader": "yarn workspace @brickdoc/uploader"
  },
  "browserslist": [
    "chrome >= 74",
    "ios >= 13",
    "firefox esr",
    "last 3 edge version",
    "last 2 safari version",
    "not dead"
  ],
  "dependencies": {
    "@apollo/client": "3.4.17",
    "@brickdoc/brickdoc-headless-design-system": "workspace:packages/brickdoc-headless-design-system",
    "@brickdoc/design-system": "workspace:packages/brickdoc-design-system",
    "@brickdoc/editor": "workspace:packages/brickdoc-editor",
    "@brickdoc/formula": "workspace:packages/brickdoc-formula",
    "@brickdoc/test-helper": "workspace:packages/brickdoc-test-helper",
    "@rails/actioncable": "^6.1.4",
    "@rails/activestorage": "^7.0.0-alpha2",
    "@sentry/react": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "ahooks": "^3.0.0-alpha.18",
    "dayjs": "^1.10.7",
    "graphql": "^16.0.1",
    "graphql-ruby-client": "^1.10.4",
    "history": "^5.1.0",
    "i18next": "^21.5.4",
    "i18next-http-backend": "^1.3.1",
    "i18next-multiload-backend-adapter": "^1.0.0",
    "lodash-es": "^4.17.21",
    "performant-array-to-tree": "^1.9.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.1.2",
    "react-i18next": "^11.14.3",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@brickdoc/eslint-plugin": "workspace:packages/eslint-plugin-brickdoc",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@cypress/code-coverage": "^3.9.12",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.6",
    "@types/qs": "^6.9.7",
    "@types/rails__actioncable": "^6.1.5",
    "@types/rails__activestorage": "^6.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-react": "^1.1.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.2",
    "babel-plugin-istanbul": "^6.1.1",
    "commitizen": "^4.2.4",
    "cypress": "^9.1.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-real-events": "^1.6.0",
    "cypress-wait-until": "^1.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security-node": "^1.0.14",
    "fs-extra": "^10.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^27.4.3",
    "jest-junit": "^13.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.1.2",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "postcss": "^8.4.4",
    "postcss-functions": "^4.0.2",
    "postcss-less": "^5.0.0",
    "postcss-preset-env": "^7.0.1",
    "postcss-will-change": "^4.0.1",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.0-beta.11",
    "vite-plugin-pwa": "^0.11.10",
    "vite-plugin-ruby": "^3.0.3",
    "yargs": "^17.3.0"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "@rails/actioncable": "patch:@rails/actioncable@npm:6.1.4#.yarn/patches/@rails-actioncable-npm-6.1.4-0b7207789c",
    "@tiptap/extension-bubble-menu": "2.0.0-beta.25",
    "@tiptap/extension-code": "2.0.0-beta.15",
    "@tiptap/extension-code-block": "2.0.0-beta.18",
    "@tiptap/extension-floating-menu": "2.0.0-beta.19",
    "@tiptap/extension-horizontal-rule": "2.0.0-beta.18",
    "@tiptap/extension-paragraph": "2.0.0-beta.16",
    "@tiptap/extension-placeholder": "2.0.0-beta.26",
    "@tiptap/suggestion": "2.0.0-beta.61",
    "vite-plugin-mdx": "patch:vite-plugin-mdx@npm:3.5.8#.yarn/patches/vite-plugin-mdx-npm-3.5.8-357f666ad6",
    "preact": "10.6.2"
  }
}
