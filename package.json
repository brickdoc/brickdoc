{
  "name": "@brickdoc/monorepo",
  "version": "0.0.0",
  "author": "Brickdoc LTD",
  "description": "Brickdoc Monorepo Root",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*",
    "tools/*"
  ],
  "scripts": {
    "postinstall": "husky install && ./bin/pdftron static && yarn api prisma generate",
    "start": "./bin/vite dev",
    "dist": "./bin/vite build",
    "dist:prod": "RAILS_ENV=production yarn dist",
    "dist:test": "RAILS_ENV=test yarn dist",
    "analyze": "BUNDLE_ANALYZER=1 yarn dist:prod",
    "dev": "ts-node tools/devkit/src/index.ts",
    "lint": "yarn lint:eslint && yarn lint:rubocop && yarn lint:brakeman",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:rubocop:fix",
    "lint:eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "lint:eslint:fix": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./",
    "lint:rubocop": "bundle exec rubocop --parallel --config .rubocop.yml",
    "lint:rubocop:fix": "bundle exec rubocop --config .rubocop.yml -a",
    "lint:brakeman": "bundle exec brakeman --no-pager --exit-on-warn",
    "tsc:check": "tsc --build",
    "test": "yarn tsc:check && yarn lint && yarn test:jest && yarn test:rspec",
    "test:jest": "jest",
    "test:cypress": "cypress open",
    "test:rspec": "bundle exec rspec",
    "commit": "git add . && cz",
    "graphql": "bundle exec rails graphql:schema:dump && graphql-codegen --config codegen.yml",
    "cleanup": "rm -rf dist packages/*/dist apps/*/dist tools/*/dist public/esm-bundle junit-reports node_modules/.cache coverage",
    "schema": "yarn workspace @brickdoc/schema",
    "design-system": "yarn workspace @brickdoc/design-system",
    "design-icons": "yarn workspace @brickdoc/design-icons",
    "editor": "yarn workspace @brickdoc/editor",
    "formula": "yarn workspace @brickdoc/formula",
    "uploader": "yarn workspace @brickdoc/uploader",
    "api": "yarn workspace @brickdoc/server-api",
    "grpc": "grpc_tools_ruby_protoc -I apps/server-api/src/anti-corruption --ruby_out=lib --grpc_out=lib grpc/main.proto"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@brickdoc/active-support": "workspace:^",
    "@brickdoc/design-system": "workspace:^",
    "@brickdoc/editor": "workspace:^",
    "@brickdoc/formula": "workspace:^",
    "@brickdoc/schema": "workspace:^",
    "@brickdoc/test-helper": "workspace:^",
    "@rails/actioncable": "^7.0.1",
    "@rails/activestorage": "^7.0.1",
    "@sentry/integrations": "^6.18.1",
    "@sentry/react": "^6.18.1",
    "@sentry/tracing": "^6.18.1",
    "dayjs": "^1.10.8",
    "framer-motion": "^6.2.8",
    "graphql": "^16.3.0",
    "graphql-ruby-client": "^1.10.6",
    "i18next": "^21.6.13",
    "i18next-http-backend": "^1.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-i18next": "^11.15.5",
    "react-router-dom": "^6.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@brickdoc/eslint-plugin": "workspace:^",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@cypress/code-coverage": "^3.9.12",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.150",
    "@swc/helpers": "^0.3.6",
    "@swc/jest": "^0.2.20",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^17.0.21",
    "@types/rails__actioncable": "^6.1.6",
    "@types/rails__activestorage": "^7.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "babel-plugin-istanbul": "^6.1.1",
    "browserlist": "^1.0.1",
    "commitizen": "^4.2.4",
    "cypress": "^9.5.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-real-events": "^1.7.0",
    "cypress-wait-until": "^1.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security-node": "^1.1.1",
    "fs-extra": "^10.0.1",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "jest-junit": "^13.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.5",
    "mocha": "^9.2.1",
    "mocha-junit-reporter": "^2.0.2",
    "postcss": "^8.4.7",
    "postcss-less": "^6.0.0",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.6",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-ruby": "^3.0.8"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "preact": "10.6.6",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@storybook/builder-webpack4": "npm:storybook-builder-vite@>=0.1.16",
    "@storybook/manager-webpack4": "npm:@storybook/manager-webpack5@>=6.5.0-alpha.40",
    "webpack": ">=5",
    "vite-plugin-mdx": "patch:vite-plugin-mdx@npm:3.5.10#.yarn/patches/vite-plugin-mdx-npm-3.5.10-ae54c30668",
    "esbuild": "^0.14.23",
    "@rails/actioncable": "patch:@rails/actioncable@npm:7.0.1#.yarn/patches/@rails-actioncable-npm-7.0.1-7dd9553527",
    "cached-path-relative": ">=1.1.0",
    "trim": ">= 1.0.1",
    "postcss": ">=8.4.6",
    "dayjs": "^1.10.7",
    "cpy": "npm:@brickdoc/cpy@>=8.1.2-patch.1",
    "@jest/transform": "npm:@jest/transform@>=27"
  }
}
