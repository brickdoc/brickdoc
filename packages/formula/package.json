{
  "name": "@mashcard/formula",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "sideEffects": false,
  "dependencies": {
    "@mashcard/active-support": "workspace:^",
    "@mashcard/design-system": "workspace:^",
    "@mashcard/schema": "workspace:^",
    "axios": "^0.27.2",
    "chevrotain": "^10.1.2",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react-vega": "^7.6.0",
    "vega": "^5.22.1",
    "vega-lite": "^5.3.0",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@mashcard/dev-support": "workspace:^",
    "@types/lodash": "^4.14.182",
    "@types/qrcode.react": "^1.0.2"
  },
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:type",
    "lint:eslint": "TIMING=1 run -T eslint --cache --ext ts,tsx src",
    "lint:fix": "TIMING=1 run -T eslint --cache --ext ts,tsx src --fix",
    "lint:type": "run -T tsc",
    "test:jest": "run -T jest",
    "test:jest:fix": "run -T jest --silent -u",
    "tsdoc": "run -T typedoc --options ../../packages/dev-support/typedoc.json --externalPattern './src/functions/**/*.ts' --out ./docs/api --entryPoints ./src/index.ts"
  }
