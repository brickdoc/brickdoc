{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "lint:eslint": {
      "outputs": []
    },
    "lint:type": {
      "outputs": ["dist/tsconfig.tsbuildinfo"]
    },
    "test:jest": {
      "outputs": ["coverage/**"]
    },
    "tsdoc": {
      "outputs": ["docs/api/**"]
    },
    "precommit": {
      "dependsOn": [
        "lint:eslint",
        "lint:type"
      ]
    }
  }
}
