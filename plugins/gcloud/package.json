{
  "name": "@brickdoc/plugin-gcloud",
  "version": "0.1.0",
  "description": "Google Cloud Plugin for Brickdoc",
  "publisher": "brickdoc",
  "engines": {
    "brickdoc": ">=0.0.1"
  },
  "activationEvents": [
    "core.logger:transport",
    "common.kms:decoder",
    "common.blob:adapter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brickdoc/brickdoc-ce.git"
  },
  "bugs": {
    "url": "https://github.com/brickdoc/brickdoc-ce/labels/active-support"
  },
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:type",
    "lint:eslint": "TIMING=1 run -T eslint --cache --ext ts,tsx src",
    "lint:fix": "TIMING=1 run -T eslint --cache --ext ts,tsx src --fix",
    "lint:type": "run -T tsc"
  },
  "sideEffects": false,
  "devDependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/testing": "^8.4.4"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^5.2.8",
    "@google-cloud/error-reporting": "^2.0.5",
    "@google-cloud/kms": "^2.11.1",
    "@google-cloud/logging-winston": "^4.2.3",
    "@google-cloud/profiler": "^4.1.7",
    "@google-cloud/storage": "^5.19.3",
    "@google-cloud/trace-agent": "^5.1.6"
  }
}
