{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "globalDependencies": ["$CI"],
  "pipeline": {
    "lint:eslint": {
      "outputs": [".eslintcache"]
    },
    "lint:fix": {
      "outputs": [".eslintcache"]
    },
    "lint:type": {
      "outputs": ["dist/tsconfig.tsbuildinfo"]
    },
    "test:jest": {
      "outputs": ["coverage/**", "junit-reports/jest.xml"]
    },
    "tsdoc": {
      "outputs": ["docs/api/**"]
    },
    "build:rust": {
      "outputs": ["*.node", "index.d.ts", "index.js"]
    }
  }
}
