{
  "name": "@brickdoc/design-colors",
  "version": "0.1.0",
  "author": "Brickdoc LTD",
  "homepage": "https://brickdoc.org",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.0.0",
  "sideEffects": false,
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json && esbuild src/less-plugin.ts --bundle --outfile=dist/less-plugin.js",
    "build:onDemand": "test -f \"dist/less-plugin.js\" || run build",
    "cleanup": "tsc --build tsconfig.build.json --clean && rm -rf dist",
    "eslint:fix": "eslint --fix --cache ./src"
  },
  "devDependencies": {
    "esbuild": "^0.14.3"
  }
}
