{
  "name": "@mashcard/dev-support",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:type",
    "lint:eslint": "TIMING=1 run -T eslint --cache --ext ts,tsx .",
    "lint:fix": "TIMING=1 run -T eslint --cache --ext ts,tsx . --fix",
    "lint:type": "run -T tsc",
    "vite:start": "vite ../../apps/client-web/src --config vite.config.ts",
    "vite:build": "vite build ../../apps/client-web/src --config vite.config.ts",
    "vite:optimize": "vite optimize ../../apps/client-web/src --config vite.config.ts",
    "vite:preview": "vite preview ../../apps/client-web/src --config vite.config.ts",
    "depcheck": "./bin/depcheck",
    "generate-vscode-workspace": "./bin/generate-vscode-workspace.js",
    "copy-pdftron-assets": "./bin/copy-pdftron-assets.js",
    "run-dev-deps": "docker-compose -f ./dev-deps-compose.yml up"
  },
  "sideEffects": false,
  "devDependencies": {
    "@vitejs/plugin-react": "^2.0.0",
    "depcheck": "^1.4.3",
    "rollup-plugin-visualizer": "^5.7.0",
    "vite": "^3.0.0",
    "vite-plugin-pwa": "^0.12.3"
  }
}
