{
  "name": "@brickdoc/schema",
  "version": "0.0.1",
  "description": "Brickdoc schema",
  "license": "GPL-3.0-or-later WITH Classpath-exception-2.0",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "devDependencies": {
    "@brickdoc/eslint-plugin": "workspace:^"
  },
  "scripts": {
    "lint": "yarn lint:eslint && yarn lint:type",
    "lint:eslint": "TIMING=1 run -T eslint --cache --ext ts,tsx src",
    "lint:fix": "TIMING=1 run -T eslint --cache --ext ts,tsx src --fix",
    "lint:type": "run -T tsc",
    "test:jest": "run -T jest"
  }
}
