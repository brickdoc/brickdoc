{
  "name": "@brickdoc/server-api",
  "private": true,
  "scripts": {
    "start": "node-dev --enable-source-maps --inspect --notify=false src/main.ts | pino-pretty -t SYS:standard -c -l",
    "start:dist": "node --enable-source-maps  dist/main.js",
    "build": "run -T swc src -s -d ./dist -C module.type=commonjs",
    "prisma": "ts-node --swc ./src/dotenvBin.ts prisma"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@nestjs/testing": "^8.2.6",
    "cross-spawn": "^7.0.3",
    "leaked-handles": "^5.2.0",
    "node-dev": "^7.1.0",
    "pino-pretty": "^7.5.1"
  },
  "dependencies": {
    "@brickdoc/active-support": "workspace:^",
    "@brickdoc/cypherpunk": "workspace:^",
    "@brickdoc/dotenv": "workspace:^",
    "@grpc/grpc-js": "^1.5.5",
    "@grpc/proto-loader": "^0.6.9",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.7",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^10.0.2",
    "@nestjs/platform-fastify": "^8.2.6",
    "@nestjs/terminus": "^8.0.4",
    "@prisma/client": "^3.9.2",
    "apollo-server-fastify": "^3.6.3",
    "fastify-cookie": "^5.5.0",
    "fastify-helmet": "^7.0.1",
    "fastify-secure-session": "github:brickdoc/fastify-secure-session",
    "graphql": "^16.3.0",
    "nestjs-pino": "^2.5.0",
    "pino-http": "^6.6.0",
    "prisma": "^3.9.2",
    "redis": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  }
}
