{
  "name": "@brickdoc/build-support",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later WITH Classpath-exception-2.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "files": [
    "dist"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "build:client-web": "vite build --config src/vite.config.ts",
    "lint": "yarn lint:eslint && yarn lint:type",
    "lint:eslint": "TIMING=1 run -T eslint --cache --ext ts,tsx src",
    "lint:fix": "TIMING=1 run -T eslint --cache --ext ts,tsx src --fix",
    "lint:type": "run -T tsc",
    "test:jest": "run -T jest",
    "test:jest:fix": "run -T jest --silent -u"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.4.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.5",
    "@vitejs/plugin-react": "^1.3.2",
    "less": "^4.1.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "unplugin-swc": "^1.3.2",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.0"
  }
}
